{
  "name": "@svelteuse/router",
  "version": "0.1.0",
  "description": "Useful svelte router helper",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "test": "c8 --reporter=lcov --reporter=text --all --include src uvu -r sucrase/register tests",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "dev": "npm run build -- --watch",
    "build": "tsup ./src/index.ts --dts --format cjs,esm --out-dir dist",
    "build:clean": "rimraf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svelteuse/router.git"
  },
  "keywords": [
    "svelte",
    "router",
    "typescript",
    "svelteuse"
  ],
  "author": "Alexander Niebuhr <45965090+alexanderniebuhr@users.noreply.github.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/svelteuse/router/issues"
  },
  "homepage": "https://github.com/svelteuse/router#readme",
  "devDependencies": {
    "@types/node": "15.0.1",
    "c8": "7.7.2",
    "rimraf": "3.0.2",
    "sucrase": "3.18.1",
    "svelte": "3.38.2",
    "ts-standard": "10.0.0",
    "tsup": "4.10.1",
    "typescript": "4.2.4",
    "uvu": "0.5.1"
  }
}
