{
  "name": "@svelteuse/router",
  "version": "0.2.6",
  "description": "useful preprocessor to generate client-side js router",
  "keywords": [
    "router",
    "svelte",
    "svelteuse",
    "typescript"
  ],
  "homepage": "https://github.com/svelteuse/router#readme",
  "bugs": {
    "url": "https://github.com/svelteuse/router/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svelteuse/router.git"
  },
  "license": "ISC",
  "author": {
    "name": "Alexander Niebuhr",
    "email": "tech@nbhr.io"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "types": "./types/node/node.d.ts",
      "import": "./dist/node/node.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --format=esm --outfile=dist/index.mjs --bundle",
    "build:clean": "rimraf dist && npm run build:old",
    "build:old": "tsup src/index.ts src/node/node.ts --format cjs,esm --target es2020",
    "dev": "npm run build -- --watch --sourcemap",
    "prepublishOnly": "npm run build:clean",
    "test": "c8 --reporter=lcov --reporter=text --all --include src uvu -r sucrase/register tests",
    "types": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "2.24.4",
    "@nbhr/utils": "0.0.29",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.4",
    "@types/babel__core": "7.1.19",
    "@types/node": "18.7.23",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "babel-plugin-polyfill-corejs3": "0.6.0",
    "browserslist": "4.21.4",
    "c8": "7.12.0",
    "core-js": "3.25.3",
    "esbuild": "0.15.10",
    "eslint": "8.24.0",
    "eslint-config-standard": "17.0.0",
    "eslint-define-config": "1.7.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-unicorn": "43.0.2",
    "regenerator-runtime": "0.13.9",
    "regexparam": "2.0.1",
    "rimraf": "3.0.2",
    "sucrase": "3.27.0",
    "svelte": "3.50.1",
    "tsup": "6.2.3",
    "typescript": "4.8.4",
    "uvu": "0.5.6"
  },
  "packageManager": "pnpm@7.12.2",
  "engines": {
    "node": "18.9.0",
    "pnpm": "7.12.2"
  },
  "publishConfig": {
    "access": "public"
  }
}