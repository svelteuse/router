{
  "name": "@svelteuse/router",
  "version": "0.2.0",
  "description": "Useful svelte router helper",
  "keywords": [
    "svelte",
    "router",
    "typescript",
    "svelteuse"
  ],
  "homepage": "https://github.com/svelteuse/router#readme",
  "bugs": {
    "url": "https://github.com/svelteuse/router/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svelteuse/router.git"
  },
  "license": "ISC",
  "author": "Alexander Niebuhr <45965090+alexanderniebuhr@users.noreply.github.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./node": {
      "require": "./dist/node/node.js",
      "import": "./dist/node/node.mjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "node": [
        "./dist/node.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "c8 --reporter=lcov --reporter=text --all --include src uvu -r sucrase/register tests",
    "dev": "npm run build -- --watch --sourcemap",
    "build:try": "tsup src/index.ts src/node/node.ts --dts --format cjs,esm --target chrome89 --target edge89 --target firefox87 --target safari13 && tsup src/placeholder.ts src/node/node.ts --dts --format cjs,esm --target es2020 && npx rimraf  dist/placeholder.js dist/placeholder.mjs dist/placeholder.d.ts",
    "build": "tsup src/index.ts src/node/node.ts --dts --format cjs,esm --target es2020",
    "build:clean": "rimraf dist && npm run build",
    "prepublishOnly": "npm run build:clean"
  },
  "devDependencies": {
    "@alexanderniebuhr/style": "1.0.13",
    "@nbhr/utils": "0.0.29",
    "@types/node": "16.11.6",
    "c8": "7.10.0",
    "regexparam": "2.0.0",
    "rimraf": "3.0.2",
    "sucrase": "3.20.3",
    "svelte": "3.44.0",
    "tsup": "4.13.1",
    "typescript": "4.4.4",
    "uvu": "0.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "16.13.0"
  }
}
