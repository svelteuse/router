{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES2017",
    "importsNotUsedAsValues": "error",
    "lib": [
      "ES2020.String"
    ],
    "types": [
      "svelte"
    ],
    // "sourceMap": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true
  }
}
